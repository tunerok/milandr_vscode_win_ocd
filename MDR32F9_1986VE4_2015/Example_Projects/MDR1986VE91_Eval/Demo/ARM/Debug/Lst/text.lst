N/*******************************************************************************
N* Function Name  : LCD_PUT_BYTE
N* Description    : Outputs one byte to the given screen coordinates.
N* Input          :     - x-coordinate
N*                : y    - y-coordinate
N*                : data - byte to output
N* Output         : None
N* Return         : None
N*******************************************************************************/
Nvoid LCD_PUT_BYTE(uint8_t x, uint8_t y, uint8_t data)
N{
N  uint32_t tmp_data, page, shift_num, shift_num_ex;
N
N  if ((x>MAX_X)|(y>MAX_Y))
X  if ((x>127)|(y>63))
N  {
N    return;
N  }
N
N  /* Select crystal and x offset */
N  SetCrystal((LCD_Crystal)(x/64));
N  x %= 64;
N  /* Determine first page and y offset */
N  page = y/8;
N  shift_num = y%8;
N  shift_num_ex = 8 - shift_num;
N
N  /* First page */
N
N  /* Read current value */
N  LCD_SET_PAGE(page);
X  WriteLCD_Cmd(0xB8 | page);
N  LCD_SET_ADDRESS(x);
X  WriteLCD_Cmd(0x40 | x);
N  tmp_data = ReadLCD_Data();
N  /* Write modified value */
N  LCD_SET_PAGE(page);
X  WriteLCD_Cmd(0xB8 | page);
N  LCD_SET_ADDRESS(x);
X  WriteLCD_Cmd(0x40 | x);
N  switch (CurrentMethod)
N  {
N    case MET_OR:
N      WriteLCD_Data(tmp_data | (data << shift_num));
N      break;
N    case MET_XOR:
N      WriteLCD_Data(tmp_data ^ (data << shift_num));
N      break;
N    case MET_NOT_OR:
N      WriteLCD_Data(tmp_data | ((data ^ 0xFF) << shift_num));
N      break;
N    case MET_NOT_XOR:
N      WriteLCD_Data(tmp_data ^ ((data ^ 0xFF) << shift_num));
N      break;
N    case MET_AND:
N      WriteLCD_Data(tmp_data & (0xFF >> shift_num_ex) | (data << shift_num));
N      break;
N  }
N
N  /* Second page (if any) */
N  if (shift_num > 0)
N  {
N    /* Read current value */
N    LCD_SET_PAGE(page+1);
X    WriteLCD_Cmd(0xB8 | page +1);
N    LCD_SET_ADDRESS(x);
X    WriteLCD_Cmd(0x40 | x);
N    tmp_data = ReadLCD_Data();
N    /* Write modified value */
N    LCD_SET_PAGE(page+1);
X    WriteLCD_Cmd(0xB8 | page +1);
N    LCD_SET_ADDRESS(x);
X    WriteLCD_Cmd(0x40 | x);
N    switch (CurrentMethod)
N    {
N      case MET_OR:
N        WriteLCD_Data(tmp_data | (data >> shift_num_ex));
N        break;
N      case MET_XOR:
N        WriteLCD_Data(tmp_data ^ (data >> shift_num_ex));
N        break;
N      case MET_NOT_OR:
N        WriteLCD_Data(tmp_data | ((data ^ 0xFF) >> shift_num_ex));
N        break;
N      case MET_NOT_XOR:
N        WriteLCD_Data(tmp_data ^ ((data ^ 0xFF)>> shift_num_ex));
N        break;
N      case MET_AND:
N        WriteLCD_Data(tmp_data & (0xFF << shift_num) | (data >> shift_num_ex));
N        break;
N    }
N  }
N}
