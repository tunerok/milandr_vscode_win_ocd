N/*----------------- Printing of symbols and strings using current font -----------------*/
N
N/*******************************************************************************
N* Function Name  : LCD_PUTC
N* Description    : Prints one symbol to the given screen coordinates.
N* Input          :   - x-coordinate
N*                : y  - y-coordinate
N*                : ch - symbol to print
N* Output         : None
N* Return         : None
N*******************************************************************************/
Nvoid LCD_PUTC(uint8_t x, uint8_t y, uint8_t ch)
N{
N  uint32_t i, j, line;
N  ucint8_t *sym;
N
N  sym = Get_Char_Data_Addr(ch);
X  sym = (CurrentFont)->pData + (ch) * (CurrentFont)->Width * ((((CurrentFont)->Height % 8) != 0) ? (1 + (CurrentFont)->Height / 8) : ((CurrentFont)->Height / 8));
N  line = CurrentFont->Height / 8;
N  if (CurrentFont->Height % 8)
N  {
N    line++;
N  }
N
N  for (j = 0; j < line; j++)
N  {
N    for( i = 0; i < CurrentFont->Width; i++)
N    {
N      LCD_PUT_BYTE(x + i, y + j*8, sym[i + CurrentFont->Width*j]);
N    }
N  }
N}
N
N/*******************************************************************************
N* Function Name  : LCD_PUTS
N* Description    : Prints the string to the given screen coordinates.
N* Input          :   - x-coordinate
N*                : y  - y-coordinate
N*                : str - pointer to string
N* Output         : None
N* Return         : None
N*******************************************************************************/
Nvoid LCD_PUTS(uint8_t x, uint8_t y, ucint8_t* str)
N{
N  uint32_t i;
N
N  for (i=0; str[i]; i++)
N  {
N    LCD_PUTC(x + i*CurrentFont->Width, y, str[i]);
N  }
N}
N
N/*******************************************************************************
N* Function Name  : LCD_PUTS_Ex
N* Description    : Prints the string using the dedicated style.
N* Input          :   - x-coordinate
N*                : y  - y-coordinate
N*                : str - pointer to string
N*                : style - style
N* Output         : None
N* Return         : None
N*******************************************************************************/
Nvoid LCD_PUTS_Ex(uint8_t x, uint8_t y, ucint8_t* str, uint8_t style)
N{
N  uint32_t i, DelayCount;
N
N  LCD_Method OldMethod = CurrentMethod;
N
N  /* Trimming results in animation speed changes */
N  DelayCount = (ADC1_GetResult() & ADC_RESULT_Msk) + 51;
X  DelayCount = (ADC1_GetResult() & ((uint32_t)0x00000FFF)) + 51;
N
N  switch (style)
N  {
N    /* Simple string */
N    case StyleSimple:
N      CurrentMethod = MET_AND;
N      LCD_PUTS(x, y, str);
N      break;
N    /* Blinking strings */
N    case StyleBlink:
N      CurrentMethod = MET_AND;
N      LCD_PUTS(x, y, str);
N      SysTickDelay(DelayCount);
N      for (i = 0; i < strlen((const char *)str); i++)
N        LCD_PUTC(x + ((CurrentFont->Width) * i), y, 0x20);
N      break;
N    /* String with modified background */
N    case StyleFlipFlop:
N      CurrentMethod = MET_AND;
N      LCD_PUTS(x, y, str);
N      CurrentMethod = MET_XOR;
N      LCD_PUTS(x, y, str);
N      CurrentMethod = MET_NOT_XOR;
N      LCD_PUTS(x, y, str);
N      SysTickDelay(DelayCount * 2);
N      CurrentMethod = MET_AND;
N      LCD_PUTS(x, y, str);
N      SysTickDelay(DelayCount);
N      break;
N    /* Shiver string */
N    case StyleVibratory:
N      CurrentMethod = MET_AND;
N      LCD_PUTS(x, y, str);
N      SysTickDelay(DelayCount);
N      LCD_PUTS(x+1, y+1, str);
N      break;
N  }
N  CurrentMethod = OldMethod;
N}
